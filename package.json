{
    "homepage": ".",
    "name": "csharpfiasco.github.io",
    "version": "0.1.1",
    "private": true,
    "type": "module",
    "engines": {
        "node": "20.x",
        "npm": "10.x"
    },
    "scripts": {
        "predeploy": "npm run build",
        "start": "vite --open",
        "preview": "vite preview",
        "build": "tsc && vite build",
        "test": "vitest",
        "test:ci": "vitest --watch false",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "prettify": "prettier --write \"./**/*.{js,jsx,ts,tsx,json}\"",
        "cy:run:local": "cypress run --env \"base_url=http://localhost:4178\" --browser chrome",
        "cy:run:local:firefox": "cypress run --env \"base_url=http://localhost:4178\" --browser firefox",
        "cy:test:local": "start-server-and-test preview http-get://localhost:4178 cy:run:local",
        "cy:test:local:firefox": "start-server-and-test preview http-get://localhost:4178 cy:run:local:firefox",
        "cy:open:local": "cypress open --env \"base_url=http://localhost:4178\"",
        "cy:run:prod": "cypress run --env \"base_url=https://www.carlosmartos.com\"",
        "cy:open:prod": "cypress open --env \"base_url=https://www.carlosmartos.com\""
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "dependencies": {
        "@emotion/react": "^11.11.1",
        "react": "^18.2.0",
        "react-awesome-reveal": "^4.2.7",
        "react-dom": "^18.2.0",
        "react-easy-emoji": "1.8.1",
        "react-headroom": "^3.2.1",
        "react-router-dom": "^6.20.1",
        "react-twitter-embed": "4.0.4"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^6.1.5",
        "@testing-library/react": "^14.1.2",
        "@types/jest": "^29.5.11",
        "@types/node": "^20.10.4",
        "@types/react": "^18.2.43",
        "@types/react-dom": "^18.2.17",
        "@types/react-headroom": "^3.2.3",
        "@typescript-eslint/eslint-plugin": "^6.14.0",
        "@typescript-eslint/parser": "^6.14.0",
        "@vitejs/plugin-react": "^4.2.1",
        "cypress": "^13.6.1",
        "eslint": "^8.55.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-airbnb-typescript": "^17.1.0",
        "eslint-plugin-import": "^2.29.0",
        "eslint-plugin-jsx-a11y": "^6.8.0",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "jsdom": "^23.0.1",
        "prettier": "^3.1.1",
        "start-server-and-test": "^2.0.3",
        "typescript": "^5.3.3",
        "vite": "^5.0.7",
        "vite-plugin-eslint": "^1.8.1",
        "vite-plugin-svgr": "^4.2.0",
        "vite-tsconfig-paths": "^4.2.2",
        "vitest": "^0.34.6"
    }
}
