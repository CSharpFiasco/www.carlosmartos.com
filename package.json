{
    "homepage": ".",
    "name": "csharpfiasco.github.io",
    "version": "0.1.1",
    "private": true,
    "engines": {
        "node": "20.x",
        "npm": "10.x"
    },
    "scripts": {
        "predeploy": "npm run build",
        "start": "vite --open",
        "preview": "vite preview",
        "build": "tsc && vite build",
        "test": "vitest",
        "test:ci": "vitest --watch false",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "pretty": "prettier --write \"./**/*.{js,jsx,ts,tsx,json}\"",
        "cy:run:local": "cypress run --env \"base_url=http://localhost:4178\"",
        "cy:test:local": "start-server-and-test preview http-get://localhost:4178 cy:run:local",
        "cy:open:local": "cypress open --env \"base_url=http://localhost:4178\"",
        "cy:run:prod": "cypress run --env \"base_url=https://www.carlosmartos.com\"",
        "cy:open:prod": "cypress open --env \"base_url=https://www.carlosmartos.com\""
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "dependencies": {
        "@emotion/react": "^11.10.8",
        "react": "^18.2.0",
        "react-awesome-reveal": "^4.2.3",
        "react-dom": "^18.2.0",
        "react-easy-emoji": "1.8.1",
        "react-headroom": "^3.2.1",
        "react-router-dom": "^6.11.1",
        "react-twitter-embed": "4.0.4"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^6.0.0",
        "@testing-library/react": "^14.0.0",
        "@types/jest": "^29.5.1",
        "@types/node": "^20.10.0",
        "@types/react": "^18.2.0",
        "@types/react-dom": "^18.2.1",
        "@types/react-headroom": "^3.2.0",
        "@typescript-eslint/eslint-plugin": "^6.11.0",
        "@typescript-eslint/parser": "^6.11.0",
        "@vitejs/plugin-react": "^4.0.0",
        "cypress": "^13.3.0",
        "eslint": "^8.2.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-airbnb-typescript": "^17.1.0",
        "eslint-plugin-import": "^2.25.3",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "eslint-plugin-react": "^7.28.0",
        "eslint-plugin-react-hooks": "^4.3.0",
        "jsdom": "^22.1.0",
        "prettier": "^3.0.1",
        "start-server-and-test": "^2.0.0",
        "typescript": "^5.0.4",
        "vite": "^4.4.9",
        "vite-plugin-eslint": "^1.8.1",
        "vite-plugin-svgr": "^4.1.0",
        "vite-tsconfig-paths": "^4.2.0",
        "vitest": "^0.34.1"
    }
}
