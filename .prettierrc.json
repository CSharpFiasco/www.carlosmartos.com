{
  "plugins": ["prettier-plugin-astro"],
  "trailingComma": "es5",
  "tabWidth": 2,
  "printWidth": 120,
  "semi": false,
  "singleQuote": true,
  "endOfLine": "crlf",
  "overrides": [
    {
      "files": ["*.js", "*.jsx", "*.ts", "*.tsx"],
      "options": {
        "tabWidth": 2,
        "semi": true
      }
    },
    {
      "files": "*.astro",
      "options": {
        "parser": "astro",
        "semi": true
      }
    }
  ]
}
